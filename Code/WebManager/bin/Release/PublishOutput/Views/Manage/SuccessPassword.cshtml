@{
    ViewData["Title"] = "Set Password";
    string userName = ViewData["userName"] as string;
    Layout = "~/Views/Shared/_ContactLayout.cshtml";
}
<div class="page-header">
    <h1>
        Cập nhật mật khẩu 
    </h1>
</div>
<div class="alert alert-block alert-warning">
    <p>* Bạn đã cập nhật mật khẩu thành công</p>
</div>
<style>
    .control-label {
        font-weight: bold;
    }
</style>
<div class="panel panel-primary">
    <div class="panel-heading">
        <h4 class="panel-title">Thông tin đầy đủ</h4>
    </div>
    <div class="panel-body">
        <h5>Đây là thông  tin đăng nhập của bạn</h5>
        <p>Tài khoản: <strong>@userName</strong></p>
        <p>Mật khẩu: <strong>**********</strong></p>
        <p>Vui lòng truy cập tại website <a href="https://dangkythitoeic.com/">dangkythitoeic.com</a> để tiến hành đăng nhập và đăng ký ngày thi nhé</p>
    </div>
</div>
@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}
